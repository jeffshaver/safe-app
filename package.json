{
  "name": "safe-app",
  "version": "0.0.4",
  "description": "Safe App",
  "repository": "https://github.com/jeffshaver/safe-app",
  "scripts": {
    "build": "webpack",
    "bump-major": "node ./bin/bump.js major --commit --tag",
    "bump-minor": "node ./bin/bump.js minor --commit --tag",
    "bump-patch": "node ./bin/bump.js patch --commit --tag",
    "create-default-config": "echo -n \"module.exports = {\n  apiUri: 'http://localhost/api',\n  hotModuleReloadUri: 'localhost',\n  domain: 'localhost',\n  port: 80,\n  protocol: 'http'\n}\" > config.js",
    "dev-server": "node server.js",
    "lint": "eslint --ext .js,.jsx src/ test/ ./*.js",
    "prepublish": "npm run lint && npm test",
    "test": "mocha --compilers js:babel-register test/index",
    "test:watch": "npm test -- --watch"
  },
  "author": "Jeff Shaver <jeff.e.shaver@gmail.com> (http://jeffshaver.me)",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-core": "^6.7.4",
    "babel-eslint": "6.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-dev-warning": "^0.1.0",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.7.2",
    "babel-runtime": "^6.6.1",
    "deep-freeze": "0.0.1",
    "eslint": "~2.5.3",
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.1.0",
    "eslint-plugin-react": "^4.2.3",
    "eslint-plugin-standard": "^1.3.2",
    "expect": "^1.16.0",
    "json-file-plus": "^3.3.0",
    "mocha": "^2.4.5",
    "nock": "^7.7.2",
    "react-transform-hmr": "^1.0.4",
    "redux-mock-store": "1.0.2",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "dependencies": {
    "color": "^0.11.1",
    "connect-mongo": "^1.1.0",
    "cookie-parser": "^1.4.1",
    "errorhandler": "^1.4.3",
    "express": "^4.13.4",
    "express-session": "^1.13.0",
    "flexboxgrid": "^6.3.0",
    "highcharts": "^4.2.3",
    "isomorphic-fetch": "^2.2.1",
    "leaflet": "^0.7.7",
    "material-ui": "^0.14.4",
    "mongodb": "^2.1.14",
    "morgan": "^1.7.0",
    "radium": "^0.17.1",
    "react": "^0.14.8",
    "react-dom": "^0.14.8",
    "react-highcharts": "^8.0.3",
    "react-leaflet": "^0.10.2",
    "react-redux": "^4.4.1",
    "react-router": "^2.0.1",
    "react-router-redux": "^4.0.0",
    "react-tap-event-plugin": "0.2.2",
    "redux": "^3.3.1",
    "redux-thunk": "^2.0.1",
    "safe-framework": "0.0.12",
    "serve-static": "^1.10.2"
  }
